{
  "name": "To do list!",
  "manifest_version": 2,
  "version": "1.0",
  "description": "My first Chrome extension.",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["style.css"],
      "js": ["app.js"]
    }
],
  "content_security_policy": "script-src 'self'  object-src 'self' blob: filesystem:",
  "browser_action": {
    "default_icon": "1list.jpg",
    "default_popup": "index.html"
  }
}
