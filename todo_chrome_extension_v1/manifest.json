{
  "name": "To do list!",
  "manifest_version": 2,
  "version": "1.0",
  "description": "My first Chrome extension.",
  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "css": ["todo.css"],
      "js": ["test.js"]
    }
],
  "content_security_policy": "script-src 'self' http://yui.yahooapis.com/3.18.1/build/yui/yui-min.js; object-src 'self' blob: filesystem:",
  "browser_action": {
    "default_icon": "list.jpg",
    "default_popup": "todo1.html"
  }
}
